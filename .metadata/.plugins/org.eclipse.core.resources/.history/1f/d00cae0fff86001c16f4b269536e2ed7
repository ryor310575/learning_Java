package tutorial.fundaciontelefonica.com;
public class tutorial2Control {
	public static void main(String[] args) {
		double max,min,random_print;
		int random_intentos,max_intentos;
		max=65;
		min=18;
		max_intentos=20;
		random_intentos=(int)Math.random()*100%2;
			for(int i=0;(i<random_intentos||i<max_intentos);i++) {
				random_print=Math.random()*100;
				if(random_print>=min&&random_print<=max) {
					System.out.println("el Numero "+i+" es "+random_print);
				}
				
			}
				
		}
}
