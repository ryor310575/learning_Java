package tutorial.fundaciontelefonica.com;

//import java.util.Scanner;

	public static void main(String[] args){
		//Scanner entrada=new Scanner(System.in);
		double max,min,max_intentos,random_print;
		int random_intentos;
		max=65;
		min=18;
		max_intentos=20;
		random_intentos=Math.random()*100%2;
		for(int i=0;(i<random_intentos&&1<max_intentos)) {
			random_print=Math.random()*100;
			if(random_print>=min&&random_print<=max) {
				Syatem.out.println("el Numero "+i+" es "+random_print)
			}
		}
			
		//entrada.close();
	}
